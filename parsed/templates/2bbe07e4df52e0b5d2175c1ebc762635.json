{"components/common/payment-icons":"{{#with theme_settings}}\r\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\r\n    <div class=\"footer-payment-icons\">\r\n        {{#if show_accept_amex}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-american-express\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_discover}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-discover\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_mastercard}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-mastercard\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_paypal}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-paypal\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_visa}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-visa\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_amazonpay}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-amazonpay\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_googlepay}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-googlepay\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_klarna}}\r\n        <svg class=\"footer-payment-icon\"><use href=\"#icon-logo-klarna\"></use></svg>\r\n        {{/if}}\r\n    </div>\r\n    {{/or}}\r\n{{/with}}\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                <a class=\"icon icon--{{display_name}}\"\r\n                   href=\"{{url}}\"\r\n                   target=\"_blank\"\r\n                   rel=\"noopener\"\r\n                   title=\"{{{capitalize display_name}}}\"\r\n                >\r\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\r\n                    <svg>\r\n                        <use href=\"#icon-{{display_name}}\"/>\r\n                    </svg>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/common/subscription-form":"<h3 class=\"footer-info-heading page-heading\" data-aos=\"fade-up\" data-aos-duration=\"500\">{{{lang 'newsletter.subscribe'}}}</h3>\r\n<p data-aos=\"fade-up\" data-aos-duration=\"5000\">{{lang 'newsletter.subscribe_intro'}}</p>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\" data-aos=\"fade-up\" data-aos-duration=\"5000\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"{{customer.email}}\"\r\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                       required\r\n                >\r\n                <button type=\"submit\" class=\"button form-prefixPostfix-button--postfix\">{{lang 'newsletter.subscribe_submit'}}</button>\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n","components/dh-custom/common/footer":"{{#if banners.bottom}}\r\n    <div class=\"banners\" data-banner-location=\"bottom\">\r\n        {{#each (limit banners.bottom_metadata 1)}}\r\n            <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                <div data-event-type=\"promotion-click\">\r\n                    {{{this.content}}}\r\n                </div>\r\n            </div>\r\n        {{/each}}\r\n    </div>\r\n{{/if}}\r\n<div class=\"footer-top\">\r\n    {{#if (hyphenate (dashcase template) '===' 'pages-home')}}\r\n    <div class=\"wave\"></div>\r\n    {{/if}}\r\n    <div class=\"object-halfRoundedBordersRight\"></div>\r\n    <div class=\"container\">\r\n        <div class=\"footer-subscription\">\r\n            {{#if settings.show_newsletter_box}}\r\n                {{> components/common/subscription-form}}\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</div>\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\r\n    <div class=\"container\">\r\n        <section class=\"footer-info\">\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\" data-aos=\"fade-up\" data-aos-duration=\"5000\" data-aos-offset=\"-200\">\r\n                <h3 class=\"footer-info-heading footer-title active\">{{lang 'footer.navigate'}}</h3>\r\n                <ul class=\"footer-info-list is-active\">\r\n                    {{#each pages}}\r\n                        {{#or (if name '===' 'About Us') (if name '===' 'Blog')}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/or}}\r\n                    {{/each}}\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\" data-aos=\"fade-up\" data-aos-duration=\"5000\" data-aos-offset=\"-200\">\r\n                <h3 class=\"footer-info-heading footer-title\">Help & Support</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/contact-us/\">Contact Us</a>\r\n                    </li>\r\n                    {{#if settings.phone_number}}\r\n                    <li>\r\n                        <a href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    <li>\r\n                        <a href=\"/shipping-returns/\" target=\"_blank\">Shipping & Returns</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/terms-of-service/\" target=\"_blank\">Terms of Service</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\" data-aos=\"fade-up\" data-aos-duration=\"5000\" data-aos-offset=\"-200\">\r\n                <h3 class=\"footer-info-heading footer-title\">{{lang 'footer.brands'}}</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each shop_by_brand}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n                </ul>\r\n            </article>\r\n            {{/and}}\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\" data-aos=\"fade-up\" data-aos-duration=\"5000\" data-aos-offset=\"-200\">\r\n                <h3 class=\"footer-info-heading footer-title\">Account</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#if customer}}\r\n                    <li>\r\n                        <a href=\"{{urls.account.index}}\" aria-label=\"{{lang 'common.account'}}\">{{lang 'common.account'}}</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"{{urls.auth.logout}}\" aria-label=\"{{lang 'common.logout'}}\">{{lang 'common.logout'}}</a>\r\n                    </li>\r\n                    {{else}}\r\n                    <li>\r\n                        <a href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">{{lang 'common.login'}}</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"{{urls.auth.create_account}}\" aria-label=\"{{lang 'common.sign_up'}}\">{{lang 'common.sign_up'}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    <li>\r\n                        <a href={{urls.account.orders.all}}>Order Status</a>\r\n                    </li>\r\n                    {{#if settings.gift_certificates_enabled}}\r\n                    <li>\r\n                        <a href=\"{{urls.gift_certificate.purchase}}\" aria-label=\"{{lang 'common.gift_cert'}}\">\r\n                            {{lang 'common.gift_cert'}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                </ul>\r\n            </article>\r\n            {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n                <article class=\"footer-info-col footer-info-col--social\" data-section-type=\"footer-webPages\" data-aos=\"fade-up\" data-aos-duration=\"5000\" data-aos-offset=\"-200\">\r\n                    <h3 class=\"footer-info-heading footer-social-title\">{{lang 'social.connect'}}</h3>\r\n                    {{> components/common/social-links}}\r\n                </article>\r\n            {{/if}}\r\n            {{!--<article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\r\n                {{> components/common/payment-icons}}\r\n            </article>--}}\r\n        </section>\r\n        \r\n    </div>\r\n    <div class=\"footer-bottom\">\r\n        <div class=\"container\">\r\n            {{#if theme_settings.show_powered_by}}\r\n                <div class=\"footer-copyright\">\r\n                    <p class=\"powered-by\">{{lang 'footer.powered_by'}} <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\r\n                </div>\r\n            {{/if}}\r\n            {{#if theme_settings.show_copyright_footer}}\r\n                <div class=\"footer-copyright\">\r\n                    <p class=\"powered-by\">Copyright &copy; {{moment format=\"YYYY\"}} {{settings.store_name}}. All rights reserved.</p><ul><li><a href=\"https://tradeshowbooth.com/pages/terms-of-service\" target=\"_blank\">Terms & Conditions</a></li><li><a href=\"https://tradeshowbooth.com/pages/privacy-policy\" target=\"_blank\">Privacy Policy</a></li></ul>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n    {{{region name=\"ssl_site_seal--global\"}}}\r\n</footer>\r\n"}
