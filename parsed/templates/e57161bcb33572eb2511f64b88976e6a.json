{"components/common/paginator":"<nav class=\"pagination\" aria-label=\"pagination\">\r\n    <ul class=\"pagination-list\">\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{previous}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.previous'}}\"\r\n                >\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-arrow-left\"></use>\r\n                        </svg>\r\n                    </i>\r\n                    {{lang 'common.previous'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each links}}\r\n            <li class=\"pagination-item desktop  {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{this.url}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   {{#if this.number '==' ../current}}\r\n                   aria-current=\"page\"\r\n                   data-pagination-current-page-link\r\n                   {{/if}}\r\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\r\n                >\r\n                    {{this.number}}\r\n                </a>\r\n            </li>\r\n            \r\n        {{/each}}\r\n        {{#if (length (JSONstringify links)) '>' 0}}\r\n        <li class=\"pagination-item pagination-item-mobile\" style=\"display:none\">\r\n           <div class=\"mobile-page\">\r\n             {{#if (length (JSONstringify links)) '>' 0}}\r\n              {{#each links}}\r\n                {{#if this.number '==' ../current}}\r\n                  <span>{{this.number}}</span>\r\n                {{/if}}\r\n                {{/each}} <span class=\"devider\">/</span><span class=\"mobile-pages-count\" ></span>\r\n             {{/if}}\r\n           </div>\r\n         </li>\r\n         {{/if}}\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{next}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.next'}}\"\r\n                >\r\n                    {{lang 'common.next'}}\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-arrow-right\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n"}
