{"components/customers-canvas/customers-canvas-cart-preview":"<img class=\"cart-item-personalize-image\" src=\"\" />\r\n{{inject \"currentLineItemId\" id}}\r\n<script>\r\n    var jsContext = JSON.parse({{ jsContext }});\r\n    console.log(\"Context is\");\r\n    console.log(jsContext);\r\n    var imageElements = document.getElementsByClassName('cart-item-personalize-image');\r\n    for (var imageElement of imageElements) {\r\n        let imgSrc = imageElement.getAttribute(\"src\");\r\n        if (imgSrc === \"\") {\r\n            imageElement.src = `https://bigcommerce-adapter.azurewebsites.net/api/bigcommerce-adapter/v1/raw-projects/preview-image?tenantId=1379&LineItemId=${jsContext.currentLineItemId}&CartId=${this.window.BCData.customersCanvasTransferObjects.CartId}&storefrontId=1397`;\r\n            break;\r\n        }\r\n    };\r\n</script>\r\n"}
